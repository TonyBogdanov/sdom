{
    "name": "tonybogdanov/sdom",
    "description": "Simple jQuery-like DOM representation in PHP",
    "type": "library",
    "require": {
        "php" : "^7.1",
        "kevintweber/html-tokenizer": "0.4",
        "symfony/css-selector": "4.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "6.5.5",
        "php-coveralls/php-coveralls": "2.0.0",
        "apigen/apigen": "dev-master",
        "roave/better-reflection": "dev-master#c87d856"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Tony Bogdanov",
            "email": "tonybogdanov@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SDom\\": "classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SDom\\Test\\": "tests/classes/"
        }
    }
}
